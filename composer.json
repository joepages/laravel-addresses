{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "joepages/laravel-addresses",
    "description": "Polymorphic addresses package for Laravel - Attach N addresses to any model",
    "keywords": ["laravel", "addresses", "polymorphic", "multi-tenancy"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/joepages/laravel-addresses",
    "authors": [
        {
            "name": "Jose Pages",
            "homepage": "https://github.com/joepages"
        }
    ],
    "support": {
        "issues": "https://github.com/joepages/laravel-addresses/issues",
        "source": "https://github.com/joepages/laravel-addresses"
    },
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Addresses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Addresses\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addresses\\AddressesServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
